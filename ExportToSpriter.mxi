<?xml version="1.0" encoding="UTF-8"?>
<macromedia-extension name="Export to Spriter" version="0.1.1" type="command">
	<author name="Pedro Chavez" />
	<products>
		<product name="Flash" version="11" primary="true" required="true" />
	</products>
	<description><![CDATA[This extension reads Flash timeline assets and exports them to the Spriter format.]]></description>
	<ui-access><![CDATA[The command can be found in 'Commands > Spriter']]></ui-access>
	<license-agreement><![CDATA[]]></license-agreement>
	<files>
		<file source="Commands/Spriter - all main elements.jsfl" destination="$flash/Commands/Spriter" />
		<file source="Commands/PNG - timeline selection.jsfl" destination="$flash/Commands/Spriter" />
		<file source="Commands/PNG - library selection.jsfl" destination="$flash/Commands/Spriter" />
		<file source="Library/png_exporter.jsfl" destination="$flash/Spriter" />
		<file source="Library/spriter_exporter.jsfl" destination="$flash/Spriter" />
		<file source="Library/debug.jsfl" destination="$flash/Spriter" />
		<file source="Library/math.jsfl" destination="$flash/Spriter" />
		<file source="Library/trig.jsfl" destination="$flash/Spriter" />
		<file source="Library/logger.jsfl" destination="$flash/Spriter" />
	</files>
</macromedia-extension>